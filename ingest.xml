<?xml version="1.0" encoding="UTF-8"?>


<processes>

    <inputfile>./yago_test.tsv</inputfile>
    <columns>
        <col>subject</col>
        <type>chararray</type>
        <col>predicate</col>
        <type>chararray</type>
        <col>object</col>
        <type>chararray</type>
    </columns>
    <delimiter>space</delimiter>

    <process>
        <name>filtered_records</name>
        <task>filter</task>
        <column>predicate</column>
        <condition>==</condition>
        <clause>&lt;isHeadOf&gt;</clause>
    </process>

    <process>
        <name>grouped_records</name>
        <task>group</task>
        <column>predicate</column>variable
    </process>

    <process>
        <name>distinct_records</name>
        <task>distinct</task>
        <table>records</table>
    </process>

    <!-- <process>
        <name>union_records</name>
        <task>union</task>
        <table>records</table>
        <table1>filtered_records</table1>
    </process> -->

    <process>
        <name>cross_records</name>
        <task>cross</task>
        <table>records</table>
        <table1>filtered_records</table1>
    </process>

    <process>
        <name>join_records</name>
        <task>join</task>
        <table>records</table>
        <column>predicate</column>
        <table1>filtered_records</table1>
        <column1>predicate</column1>
    </process>

    <process>
        <name>limit_records</name>
        <task>limit</task>
        <table>records</table>
        <clause>5</clause>
    </process>

    <process>
        <name>order_records</name>
        <task>order</task>
        <table>records</table>
        <column>subject</column>
    </process>




    <!-- <dumpvar>filtered_records</dumpvar>
    <outputfile>./yagowordcounts</outputfile> -->

    <!-- <dumpvar>grouped_records</dumpvar>
    <outputfile>./yagowordcounts</outputfile> -->

    <!-- <dumpvar>distinct_records</dumpvar>
    <outputfile>./yagowordcounts</outputfile> -->

    <!-- <dumpvar>union_records</dumpvar>
    <outputfile>./yagowordcounts</outputfile> -->

    <!-- <dumpvar>cross_records</dumpvar>
    <outputfile>./yagowordcounts</outputfile> -->

    <!-- <dumpvar>join_records</dumpvar>
    <outputfile>./yagowordcounts</outputfile> -->

    <!-- <dumpvar>limit_records</dumpvar>
    <outputfile>./yagowordcounts</outputfile> -->

    <dumpvar>order_records</dumpvar>
    <outputfile>./yagowordcounts</outputfile>


</processes>
